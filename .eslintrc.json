{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6":true
  },
  "plugins": [ "react" ],
  "parserOptions": { "ecmaFeatures": { "jsx": true } },
  "rules": {
    "react/jsx-uses-vars": 1 ,
    "indent": ["error", 2],
    "accessor-pairs": 2,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "complexity": 2,
    "consistent-return": 2,
    "curly": 2,
    "dot-notation": 2,
    "guard-for-in": 2,
    "eqeqeq": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 2,
    "no-empty-pattern": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 0,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-void": 2,
    "no-with": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "yoda": 2,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "block-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "consistent-this": 2,
    "id-blacklist": 2,
    "id-match": 2,
    "linebreak-style": 2,
    "lines-around-comment": 2,
    "max-depth": 2,
    "max-nested-callbacks": 2,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-continue": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "no-restricted-syntax": 2,
    "no-spaced-func": 2,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": ["error", "always"],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "semi": 2,
    "semi-spacing": 2,
    "sort-vars": 2,
    "space-in-parens": 2,
    "unicode-bom": 2,
    "wrap-regex": 2,
    "constructor-super": 2,
    "generator-star-spacing": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-new-symbol": 2,
    "no-restricted-imports": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-var": 2,
    "prefer-const": 2,
    "prefer-reflect": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quotes": ["error", "single"],
    "require-yield": 2,
    "template-curly-spacing": 2,
    "yield-star-spacing": 2
  }
}
